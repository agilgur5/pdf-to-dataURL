{
  "name": "pdf-to-dataURL",
  "version": "0.1.0",
  "description": "A tiny wrapper around pdf.js to convert PDFs to dataURLs",
  "main": "build/index.js",
  "dependencies": {
    "pdfjs-dist": "^1.4.237"
  },
  "devDependencies": {
    "babel-core": "^6.0.14",
